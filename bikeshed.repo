# For debugging and extra packages...
[fedora-modular-bikeshed]
name=Fedora BIKESHED $releasever - $basearch MODULAR
failovermethod=priority
baseurl=https://kojipkgs.fedoraproject.org/compose/latest-Fedora-Modular-Bikeshed/compose/Server/x86_64/os/
enabled=1
metadata_expire=6h
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-$basearch
skip_if_unavailable=False
