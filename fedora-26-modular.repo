[fedora-26-modular]
name=Fedora Modular 26 - $basearch
failovermethod=priority
baseurl=https://download.fedoraproject.org/pub/alt/unofficial/releases/26/Server/$basearch/os/
enabled=false
#metadata_expire=7d
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-modularity
skip_if_unavailable=False
modules=1
